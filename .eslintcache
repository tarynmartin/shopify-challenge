ations => [...nominations, movie])\n  } \n\n  const removeNomination = (movie) => {\n    setNominations(nominations.filter(nomination => nomination.imdbID !== movie.imdbID))\n  }\n\n  const setLocalStorage = () => {\n    console.log('got here')\n    console.log(nominations)\n    if (nominations.length === 0) {\n      console.log('here')\n      localStorage.clear()\n    } else if (nominations.length > 0) {\n      console.log('not here')\n      localStorage.setItem('nominations', JSON.stringify(nominations));\n    }\n  }\n\n  const handleValueChange = (e) => {\n    if (e.nativeEvent.inputType !== 'deleteContentBackward') {\n      setSearchValue(searchValue + e.nativeEvent.data)\n    } else if (e.nativeEvent.inputType === 'deleteContentBackward') {\n      setSearchValue(searchValue.slice(0, searchValue.length - 1))\n    }\n  }\n\n  const closeBanner = () => {\n    setPopup(false);\n  }\n\n    return (\n      <div className=\"App\">\n        <header>\n          <h1 className='header-name'>The Shoppies</h1>\n        </header>\n        {popup &&\n          <Popup close={closeBanner}/>\n        }\n        <Search setValue={handleValueChange}/>\n        <div className='results-nominations'>\n          <Results movies={movies} nominate={handleNomination} hidden={hidden}/>\n          <Nominations nominations={nominations} remove={removeNomination}/>\n        </div>\n        \n      </div>\n    )\n}\n\nexport default App;\n","/Users/taryn/shopify-challenge/src/reportWebVitals.js",[],"/Users/taryn/shopify-challenge/src/Components/Search/Search.js",[],"/Users/taryn/shopify-challenge/src/Components/Results/Results.js",[],"/Users/taryn/shopify-challenge/src/Components/Nominations/Nominations.js",[],"/Users/taryn/shopify-challenge/src/apiCalls.js",[],"/Users/taryn/shopify-challenge/src/Components/MovieCard/MovieCard.js",[],"/Users/taryn/shopify-challenge/src/Components/Popup/Popup.js",[],{"ruleId":"43","replacedBy":"44"},{"ruleId":"45","replacedBy":"46"},{"ruleId":"47","severity":1